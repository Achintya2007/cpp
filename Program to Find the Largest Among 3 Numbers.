#include <iostream>
using namespace std;
int main() {
    int a,b,c;
    cout<<"enter a value =";
    cin>>a>>b>>c;
    if(a>b && a>c){
        cout<<"a is greater then b and c";
    }
    else if(b>a && b>c){
        cout<<"b is greater then a and c";
    }
    else{
        cout<<"c is greater then a and b";
    }
   
    return 0;
}
