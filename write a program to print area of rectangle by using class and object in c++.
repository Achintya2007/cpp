#include <iostream>
using namespace std;

class Rectangle {
public:
    int length, breadth;

    void input() {
        cout << "Enter length: ";
        cin >> length;
        cout << "Enter breadth: ";
        cin >> breadth;
    }

    int area() {
        return length * breadth;
    }
};

int main() {
    Rectangle r;
    r.input();
    cout << "Area: " << r.area() << endl;
    return 0;
}
